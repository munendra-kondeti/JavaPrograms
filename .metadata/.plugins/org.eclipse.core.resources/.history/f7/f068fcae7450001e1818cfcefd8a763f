package com.kn.pattrenspractice;

public class SolidRhombusPattron {

	public static void main(String[] args) {

		for (int i = 4,k=0; i >= 0; i--,k++) {
			
			for (int j = 0; j < 9; j++) {
				if (j >= i && k<=i) {
					System.out.print("*");
				} else {
					System.out.print("_");
				}
			}
			System.out.println();
		}
	}

}
