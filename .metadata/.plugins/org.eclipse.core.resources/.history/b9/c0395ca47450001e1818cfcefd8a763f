package com.kn.pattrenspractice;

public class SolidRhombusPattron {

	public static void main(String[] args) {

		int k=4; 
		for (int i = k; i >= 0; i--) {
			
			for (int j = 0; j < 9; j++) {
				if (j >= i && k-i >=0) {
					System.out.print("*");
				} else {
					System.out.print("_");
				}
			}
			System.out.println();
		}
	}

}
