package com.kn.arrayrotation;

public class RotateArray {
	// Method
	void rotateArray(int[] arr ) {
		// {10,20,30,40,50};
//		for(int i = 0 ; i< arr.length-1; i ++) {
		int temp = arr[0];
			for(int j = 0; j < arr.length-1;j++) {
				arr[j] = arr[j+1];
				arr[j+1] = temp ;
			}
//		}
		for(int e : arr)
			System.out.println(e);
	}
}
