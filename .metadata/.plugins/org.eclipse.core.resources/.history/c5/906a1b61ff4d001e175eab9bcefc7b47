package com.kn.arrayrotation;

public class RotateArray {
	// Method
	void rotateArray(int[] arr ) {
		
		for(int i = 0 ; i< 1; i ++) {
			int temp = arr[0];
			for(int j = 0; j < arr.length;j++) {
				arr[i] = arr[i+1];
				arr[i+1] = arr[i+2]; 
			}
		}
		for(int e : arr)
			System.out.println(e);
	}
}
