package com.kn.arrayrotation;

public class RotateArray {
	// Method
	void rotateArray(int[] arr ) {
		// {10,20,30,40,50};
		for(int i = 0 ; i< arr.length; i ++) {
			for(int j = 0; j < arr.length-1;j++) {
				int temp = arr[0];
				arr[i] = arr[i+1];
				arr[i+1] = temp ;
			}
		}
		for(int e : arr)
			System.out.println(e);
	}
}
